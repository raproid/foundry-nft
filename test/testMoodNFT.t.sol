    function testFullNFTLifecycle() public {
        // Mint NFT
        vm.prank(USER);
        moodNft.mintNft();

        // Verify ownership
        assertEq(moodNft.ownerOf(0), USER);

        // Approve another user
        vm.prank(USER);
        moodNft.approve(ALICE, 0);
        assertEq(moodNft.getApproved(0), ALICE);

        // Transfer using approval
        vm.prank(ALICE);
        moodNft.transferFrom(USER, BOB, 0);

        // Verify new ownership
        assertEq(moodNft.ownerOf(0), BOB);
        assertEq(moodNft.balanceOf(USER), 0);
        assertEq(moodNft.balanceOf(BOB), 1);
    }

    function testApprovalForAll() public {
        // Mint NFTs
        vm.startPrank(USER);
        moodNft.mintNft(); // Token 0
        moodNft.mintNft(); // Token 1
        vm.stopPrank();

        // Set approval for all
        vm.prank(USER);
        moodNft.setApprovalForAll(ALICE, true);
        assertTrue(moodNft.isApprovedForAll(USER, ALICE));

        // Alice can transfer both tokens
        vm.prank(ALICE);
        moodNft.transferFrom(USER, BOB, 0);

        vm.prank(ALICE);
        moodNft.transferFrom(USER, BOB, 1);

        assertEq(moodNft.balanceOf(BOB), 2);
        assertEq(moodNft.balanceOf(USER), 0);
    }

